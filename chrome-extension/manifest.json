{
  "manifest_version": 3,
  "name": "Credit Card Generator",
  "version": "1.0",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icons/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["icons/copy.png", "icons/checkpng"],
      "matches": ["<all_urls>"]
    }
  ]
}
